<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smc.android.highlight">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/applogo"
        android:label="Highlight"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.smc.android.highlight.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.smc.android.highlight.RegisterActivity" />
        <activity android:name="com.smc.android.highlight.ChooseActivity" />
        <activity android:name="com.smc.android.highlight.MainManuActivity" />
        <activity android:name="com.smc.android.highlight.LoginActivity"></activity>
    </application>

</manifest>